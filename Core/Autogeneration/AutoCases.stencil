{% for enum in types.implementing.AutoCases|enum %}
extension {{ enum.name }} {
{% if not enum.hasAssociatedValues %}
    public static let allCases: [{{ enum.name }}] = [
{% for case in enum.cases %}        .{{ case.name }}{% if not forloop.last %},{% endif %}
{% endfor %}
    ]
{% endif %}
}
{% endfor %}
